CREATE TABLE pirnid(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
pirnisort VARCHAR (30),
r1 FLOAT,
p2 FLOAT,
r2 FLOAT,
p3 FLOAT,
r3 FLOAT
);
